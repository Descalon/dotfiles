{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#CB4B16>\u250c[</>Descalon<#CB4B16>]</>",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ .Status }} ",
          "type": "poshgit"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#CB4B16>[</>\uf0e7<#CB4B16>]</>",
          "type": "root"
        },
        {
          "foreground": "#F0C674",
          "properties": {
            "fetch_version": false
          },
          "style": "plain",
          "template": "\ue70c{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#CB4B16>[</>{{ if gt .Code 0 }}\uf00d{{ else }}\uf42e{{ end }}<#CB4B16>]</>",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "<#CB4B16>[</>{{ .CurrentDate | date .Format }}<#CB4B16>]</>",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "properties": {
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "<#CB4B16>\u2514[</>{{ .Path }}<#CB4B16>]></>",
          "type": "path"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
