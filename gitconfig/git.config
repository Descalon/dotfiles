[user]
	email = info@nicoglas.com
	name = Nico Glas
[alias]
	adog = log --all --decorate --oneline --graph
	yolo = commit --amend --no-edit -a
	nuke = !git reset HEAD --hard && git clean -fdx
[safe]
	directory = d:/*
	directory = "d:/c#/craptester"
	directory = "D:/c#/craptester"
	directory = D:/thesis/Karser
	directory = D:/thesis/*
	directory = D:/thesis/latex
[init]
	defaultBranch = main
[credential "https://git.overleaf.com"]
	provider = generic
[core]
	editor = nvim
[push]
	autoSetupRemote = true
